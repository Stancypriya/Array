public class Count_Repeated_Element {

 public static void main(String[] args) {
  // given array
  int a[] = { 1, 2, 3, 1, 2, 3 };
  // for count
  int count = 0;
  // for search
  for (int i = 0; i < a.length; i++) {
   for (int j = i + 1; j < a.length; j++) {
    if (a[i] == a[j]) {
     count++;
    }
   }
  }
  System.out.println("count of repeated element : "+ count);
 }
}
